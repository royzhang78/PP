<commands>

  <command >
    <desription>
      查询静态表信息
    </desription>
    <sqlname>findPayStaticConfigbyKey</sqlname>
    <sql><![CDATA[
		select * from sys_pay_static_config where `key`=#key#
    ]]>
    </sql>
  </command>
 
 
 
